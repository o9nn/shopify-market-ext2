{
  "name": "shopify-market-test",
  "version": "1.0.0",
  "description": "Shopify Marketplace Channel App - A Node.js/React starter kit for marketplace integrations",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "nodemon server/index.js",
    "client:dev": "cd client && npm run dev",
    "build": "cd client && npm run build",
    "start": "node server/index.js",
    "migrate": "sequelize db:migrate",
    "migrate:undo": "sequelize db:migrate:undo",
    "seed": "sequelize db:seed:all",
    "test": "jest",
    "lint": "eslint ."
  },
  "keywords": [
    "shopify",
    "marketplace",
    "channel-app",
    "ecommerce",
    "graphql"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@apollo/client": "^3.8.0",
    "@shopify/app-bridge": "^3.7.0",
    "@shopify/app-bridge-react": "^3.7.0",
    "@shopify/polaris": "^12.0.0",
    "@shopify/shopify-api": "^8.0.0",
    "axios": "^1.6.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.0",
    "express": "^4.18.2",
    "graphql": "^16.8.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.35.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.0",
    "sequelize-cli": "^6.6.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
